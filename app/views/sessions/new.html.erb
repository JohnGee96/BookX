<div id="notice">
  <%= flash.now[:alert] %>
</div>  

<div class="page-header">
  <h1>Log In</h1>
</div>
<%= form_tag sessions_path do %> 
     <div class="form-group">
       <%= label_tag :email %>
       <%= text_field :email, nil, class: "login_form-control" %>
     </div>
     <div class="form-group">
       <%= label_tag :password %>
       <%= password_field_tag :password, nil, class: "login_form-control" %>
     </div>
     <%= submit_tag "Log In", class: "btn btn-primary" %>
   <% end %>
   

